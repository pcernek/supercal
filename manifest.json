{
  "manifest_version": 3,
  "name": "Supercal",
  "version": "1.2",
  "description": "Calculates total scheduled time in Google Calendar with color breakdown",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [{
    "resources": ["icon48.png"],
    "matches": ["https://calendar.google.com/*"]
  }],
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
